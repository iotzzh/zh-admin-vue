{
  "include": ["vite.config.*"],
  "exclude": ["tsconfig.json"],
  "compilerOptions": {
    "composite": true,
    "lib": [],
    "types": ["node"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
